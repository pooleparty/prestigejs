{
  "name": "prestigejs",
  "version": "1.0.0",
  "description": "React style and component library for my personal projects",
  "main": "dist/prestige.js",
  "engines": {
    "node": ">=8.0.0",
    "npm": ">=5.0.0"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pooleparty/prestigejs.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/pooleparty/prestigejs/issues"
  },
  "homepage": "https://github.com/pooleparty/prestigejs#readme",
  "scripts": {
    "start": "webpack-dev-server --open",
    "storybook": "start-storybook -p 6006",
    "prebuild": "npm run clean:build",
    "build": "webpack",
    "predeploy": "npm run build -- -p",
    "deploy": "gh-pages -d build",
    "predist": "npm run clean:dist",
    "dist": "webpack --env=dist",
    "dist:prod": "npm run dist -- -p",
    "lint": "run-p -c lint:script lint:style",
    "lint:script": "eslint . '**/.*.js' --cache",
    "lint:style": "stylelint 'src/style/**/*.scss'",
    "lint:fix": "npm run lint:script -s -- --fix",
    "test": "jest --coverage",
    "test:watch": "jest --watchAll",
    "vrt": "backstop test --config=backstop.config.js",
    "vrt:approve": "backstop approve --config=backstop.config.js",
    "vrt:reference": "backstop reference --config=backstop.config.js",
    "clean:build": "rimraf build",
    "clean:coverage": "rimraf coverage",
    "clean:dist": "rimraf dist",
    "clean": "run-p clean:*",
    "prepush": "run-s lint",
    "prepublishOnly": "npm run dist:prod",
    "build-storybook": "build-storybook",
    "svgo": "svgo --pretty --indent=2 --folder=src/icons/ --config=.svgo.yml",
    "svgson": "svgson --pretty --title --input src/icons --output src/svgson.json",
    "precommit": "lint-staged",
    "formatAll": "prettier-eslint --write --ignore **/node_modules/** \"**/*.{js,jsx}\""
  },
  "peerDependencies": {
    "react": ">=16.0.0",
    "react-dom": ">=16.0.0"
  },
  "dependencies": {
    "@fortawesome/fontawesome": "^1.1.4",
    "classnames": "^2.2.5",
    "lodash": "^4.17.4",
    "prop-types": "^15.6.0",
    "react-feather": "^1.0.8"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.3.15",
    "@storybook/addon-info": "^3.3.15",
    "@storybook/addon-links": "^3.3.15",
    "@storybook/addons": "^3.3.15",
    "@storybook/react": "^3.3.15",
    "autoprefixer": "^7.1.4",
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^22.1.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "backstopjs": "^3.0.37",
    "copy-webpack-plugin": "^4.1.0",
    "css-loader": "^0.28.7",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.8.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.4.0",
    "extract-text-webpack-plugin": "^3.0.1",
    "gh-pages": "^1.0.0",
    "html-webpack-plugin": "^2.30.1",
    "husky": "^0.14.3",
    "jest": "^22.1.4",
    "jsx-to-string": "^1.3.0",
    "lint-staged": "^7.0.0",
    "lorem-ipsum": "^1.0.4",
    "node-sass": "^4.5.3",
    "npm-run-all": "^4.1.1",
    "postcss-loader": "^2.0.6",
    "prettier": "^1.11.1",
    "prettier-eslint-cli": "^4.7.1",
    "react": "^16.2.0",
    "react-copy-to-clipboard": "^5.0.1",
    "react-dom": "^16.2.0",
    "react-syntax-highlighter": "^7.0.0",
    "react-test-renderer": "^16.2.0",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "styled-components": "^3.2.3",
    "stylelint": "^8.1.1",
    "stylelint-config-ericmasiello": "^1.0.1",
    "svgo": "^1.0.5",
    "svgson": "^2.1.0",
    "tinycolor2": "^1.4.1",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.9.1",
    "webpack-merge": "^4.1.0"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "prettier-eslint --write",
      "eslint --fix",
      "git add"
    ]
  }
}
